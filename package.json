{
  "name": "wakacast",
  "version": "1.0.0",
  "description": "A wakatime.com extension for raycast",
  "repository": "https://github.com/Matt-Gleich/wakacast.git",
  "author": "Matthew Gleich <git@mattglei.ch>",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "publish": "ray archive",
    "dev": "ray develop",
    "format": "prettier -w .",
    "format::check": "prettier --check ."
  },
  "raycastCommand": {
    "title": "View Wakatime Languages",
    "packageName": "Wakatime",
    "icon": "clock",
    "type": "view",
    "debug": {
      "reloadShortcut": {
        "modifiers": [
          "cmd",
          "opt"
        ],
        "key": "r"
      }
    }
  },
  "dependencies": {
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
