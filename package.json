{
  "name": "emojicast",
  "version": "1.0.0",
  "description": "A emoji selector for raycast",
  "author": "Matthew Gleich <git@mattglei.ch>",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "publish": "ray archive",
    "dev": "ray develop",
    "format": "prettier -w .",
    "format::check": "prettier --check .",
    "lint": "eslint ."
  },
  "keywords": [
    "emojis",
    "search",
    "copy and paste"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Matt-Gleich/emojicast"
  },
  "raycastCommand": {
    "title": "Copy an Emoji",
    "packageName": "Emojis",
    "icon": "ðŸš€",
    "type": "view",
    "debug": {
      "reloadShortcut": {
        "modifiers": [
          "cmd",
          "opt"
        ],
        "key": "r"
      }
    }
  },
  "dependencies": {
    "@types/raycast-commands": "file:./api",
    "emoji.json": "^13.1.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.2.1"
  }
}
