{
  "name": "emojicast",
  "title": "Copy an Emoji",
  "icon": "ðŸš€",
  "version": "1.0.0",
  "description": "An emoji selector for raycast",
  "author": "Matthew Gleich <email@mattglei.ch>",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "publish": "ray archive",
    "dev": "ray develop",
    "format": "prettier -w .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "gen": "cd scripts && go run gen.go && cd .. && yarn run format"
  },
  "keywords": [
    "emojis",
    "search",
    "copy and paste"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Matt-Gleich/emojicast"
  },
  "commands": [
    {
      "name": "index",
      "title": "Copy an Emoji",
      "description": "Copy an emoji to your system clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@types/raycast-commands": "file:./api",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.2.1"
  }
}
