{
  "name": "wakacast",
  "version": "1.0.0",
  "description": "A wakatime.com extension for raycast",
  "author": "Matthew Gleich <git@mattglei.ch>",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "publish": "ray archive",
    "dev": "ray develop",
    "format": "prettier -w .",
    "format::check": "prettier --check ."
  },
  "keywords": [
    "wakatime",
    "languages",
    "time"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Matt-Gleich/wakacast"
  },
  "raycastCommand": {
    "title": "View Your Wakatime Languages",
    "packageName": "Wakatime",
    "icon": "‚è∞",
    "type": "view",
    "debug": {
      "reloadShortcut": {
        "modifiers": [
          "cmd",
          "opt"
        ],
        "key": "r"
      }
    }
  },
  "dependencies": {
    "@types/raycast-commands": "file:./api",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.2.1"
  }
}
